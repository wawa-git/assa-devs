-- -----------------------------------------------------
-- Table `SW_USER_ROLE`
-- -----------------------------------------------------
CREATE  TABLE  `SW_USER_ROLE` CONSTRAINT `PK_USER_ROLE` ADD  PRIMARY KEY (`USER_ROLE_ID`) ,

CREATE  TABLE  `SW_USER_ROLE` CONSTRAINT `FK_ROLE` ADD FOREIGN KEY (`ROLE_ID` )
    REFERENCES `SW_ROLE` (`ROLE_ID` );
    
CREATE  TABLE  `SW_USER_ROLE` CONSTRAINT `FK_EMPLOYEE` ADD FOREIGN KEY (`EMPL_ID` )
    REFERENCES `SW_EMPLOYEES` (`EMPL_ID` );
    
CREATE  TABLE  `SW_USER_ROLE` CONSTRAINT `FK_BU` ADD FOREIGN KEY (`BU_ID` )
    REFERENCES `SW_BU` (`BU_ID` );

