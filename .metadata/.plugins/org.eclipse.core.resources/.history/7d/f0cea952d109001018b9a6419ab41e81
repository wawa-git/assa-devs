delimiter $$

CREATE TABLE SW_ACTIVITY (
  PROJECT_ID varchar(10) NOT NULL,
  ACTIVITY_ID varchar(10) NOT NULL,
  STATUS varchar(1) DEFAULT NULL,
  DESCR varchar(45) DEFAULT NULL,
  BEGIN_DT datetime DEFAULT NULL,
  END_DT datetime DEFAULT NULL,
  MANAGER varchar(10) DEFAULT NULL,
  TEAM_ID varchar(10) DEFAULT NULL,
  PRIMARY KEY (PROJECT_ID,ACTIVITY_ID),
  
) ENGINE=MyISAM DEFAULT CHARSET=latin1$$

